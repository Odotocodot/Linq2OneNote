{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Odotocodot.OneNote.Linq/*.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "outputFormat": "mref",
      "namespaceLayout": "flattened",
      "memberLayout": "samePage", 
      "shouldSkipMarkup": false,
      "useCompatibilityFileName": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "LINQ to OneNote",
      "_appFaviconPath": "images/favicon.ico",
      "_appLogoPath": "images/logo55x48.svg",
      "_appName": "LINQ to OneNote",
      "_enableSearch": true,
      "_disableContribution": true,
      "_disableBreadcrumb" : false,
      "_disableNavbar": false,
      "_disableAffix": false,
      "_appFooter": "Â© 2023-2025 Odotocodot   |   Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a>"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "sitemap": {
      "baseUrl": "https://odotocodot.github.io/Linq2OneNote/",
      "options": {
        "changefreq": "yearly",
        "priority": 0.8
      }
    },
    "postProcessors": ["ExtractSearchIndex"],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "exportViewModel": false,
    "noLangKeyword": false,
    "markdownEngineName": "markdig",
    "cleanupCacheHistory": false
  }
}