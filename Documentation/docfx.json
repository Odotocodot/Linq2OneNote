{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Odotocodot.OneNote.Linq/*.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "LINQ to OneNote",
      "_appName": "LINQ to OneNote",
      "_enableSearch": true,
      "_disableSideFilter": false,
      "_disableNavbar" : false,
      "_disableAffix" : false,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_disableBreadcrumb": false
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}